#include<stdio.h>
#include<ctype.h>
int main(){
char ch;
printf("enter a character");
scanf("%c",&ch);
if (isalpha(ch))
{printf("Alphabet");
    if (islower(ch))
     printf("Lowercase");
    else printf("Uppercase");
}
else if (isdigit(ch))
    printf("Character is digit");
else printf("Character is an special character");
return 0;
}
